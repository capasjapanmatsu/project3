{
  "name": "dog-park-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "analyze": "npx vite-bundle-analyzer",
    "analyze:build": "vite build --mode analyze",
    "analyze:deps": "npx depcheck",
    "analyze:size": "npx bundlephobia@latest",
    "analyze:unused": "npx unimported",
    "test": "echo 'No tests specified' && exit 0",
    "pwa:sitemap": "node scripts/generate-sitemap.js",
    "deploy": "npm run build && git add . && git commit -m 'Deploy to Netlify' && git push origin main",
    "deploy:preview": "npm run build && netlify deploy",
    "deploy:prod": "npm run build && netlify deploy --prod",
    "netlify:env": "netlify env:list",
    "netlify:status": "netlify status",
    "netlify:open": "netlify open",
    "netlify:logs": "netlify logs",
    "setup:netlify": "npm install -g netlify-cli && netlify login && netlify link",
    "check:env": "echo 'Checking environment variables...' && node -e \"console.log('VITE_SUPABASE_URL:', process.env.VITE_SUPABASE_URL || 'Not set'); console.log('VITE_SUPABASE_ANON_KEY:', process.env.VITE_SUPABASE_ANON_KEY ? 'Set' : 'Not set');\"",
    "build:pwa": "node scripts/build-pwa.js",
    "pwa:dev": "node scripts/pwa-dev-helper.js",
    "pwa:clear-cache": "node scripts/pwa-dev-helper.js clear-cache",
    "pwa:debug": "node scripts/pwa-dev-helper.js debug",
    "pwa:checklist": "node scripts/pwa-dev-helper.js checklist",
    "pwa:icons": "node scripts/pwa-dev-helper.js generate-icons",
    "pwa:https": "node scripts/pwa-dev-helper.js https-setup",
    "pwa:all": "node scripts/pwa-dev-helper.js all",
    "dev:browser": "concurrently \"npm run dev\" \"sleep 3 && start http://localhost:3005\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run analyze:build\"",
    "browser:dev": "echo 'Use Ctrl+Shift+O to open in Simple Browser'",
    "browser:stats": "echo 'Use Ctrl+Alt+B to view bundle analysis'"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@noble/hashes": "^1.3.2",
    "@stablelib/base64": "^1.0.1",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.50.2",
    "browser-image-compression": "^2.0.2",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "qrcode.react": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "vite-plugin-webp": "^0.4.0",
    "vite-plugin-imagemin": "^0.6.1",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0",
    "critical": "^6.1.3",
    "purge-css": "^1.1.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}